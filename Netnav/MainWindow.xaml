<Window x:Class="Netnav.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Netnav"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid x:Name="StartWindow">
            <TextBox HorizontalAlignment="Left" Height="29" Margin="108,108,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="577"/>
            <Button Content="Button" HorizontalAlignment="Left" Height="37" Margin="314,176,0,0" VerticalAlignment="Top" Width="156" Click="Start"/>
        </Grid>
        <Grid  x:Name="ResultWindow" Visibility="Hidden">
            <Button Content="Button" HorizontalAlignment="Left" Height="37" Margin="311,351,0,0" VerticalAlignment="Top" Width="156"/>
        </Grid>
    </Grid>
</Window>
